    "        X.append(x)\n",
    "    \n",
    "        Y.append(funx)\n",
    "  \n",
    "      \n",
    "    for j in range(0,100):\n",
    "\n",
    "        x = x - 1/100\n",
    "    \n",
    "        if 0 < x < 1:\n",
    "            funx = (math.log(x,a))\n",
    "            funx = funx * (-1)\n",
    "\n",
    "            \n",
    "\n",
    "            X.append(x)\n",
    "           \n",
    "            Y.append(-1 * funx)\n",
    "  \n",
    "    j += 1\n",
    "   \n",
    "\n",
    "        \n",
